 CONST "Нравится" 1
 CONST "Нет оценки" 0
 CONST "Не нравится" 2
 INTEGER VAR left_score
 INTEGER VAR right_score
 "Открываем {(57974013)}"
 "Поставить фокус в левую половину ААК"
 "Документ нравится"
 DocumentText "Узнать, какая оценка поставлена документу" >>> left_score
 "Поставить фокус в правую половину ААК"
 DocumentText "Узнать, какая оценка поставлена документу" >>> right_score

 left_score РАВНО right_score IF
  'Оценка для левой и правой частей - одинаковая!' .
  'Оценка обеих частей:' left_score РАВНО "Нравится" ? ( ' Нравится.' Cat . )
  'Оценка обеих частей:' left_score РАВНО "Не нравится" ? ( ' Не нравится.' Cat . )
  'Оценка обеих частей:' left_score РАВНО "Нет оценки" ? ( ' Нет оценки.' Cat . )
 ELSE
  'Оценка для левой и правой частей - разная!' .
  'Оценка левой части:' left_score РАВНО "Нравится" ? ( ' Нравится.' Cat . )
  'Оценка левой части:' left_score РАВНО "Не нравится" ? ( ' Не нравится.' Cat . )
  'Оценка левой части:' left_score РАВНО "Нет оценки" ? ( ' Нет оценки.' Cat . )
  'Оценка правой части:' right_score РАВНО "Нравится" ? ( ' Нравится.' Cat . )
  'Оценка правой части:' right_score РАВНО "Не нравится" ? ( ' Не нравится.' Cat . )
  'Оценка правой части:' right_score РАВНО "Нет оценки" ? ( ' Нет оценки.' Cat . )
 ENDIF
;

