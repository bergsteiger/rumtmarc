 : AutoreferatDocument = {
  ExternalHandle = 1012138456
  ShortName = 'Оглавление'
  Name = 'Разъяснения Пленума Высшего Арбитражного Суда РФ о практике применения Федерального закона "О несостоятельности (банкротстве)"'
  LineSpacing = 100
  Style = -1
  Children = 5
  : = {
   Handle = 1
   Segments = {
    : = {
     Handle = View
     : = {
      Style = -10
      Finish = 207
     }
    }
   }
   FirstIndent = 0
   Text = '<60>ASCII4>Постановление<60>ASCII4>12038456<60>ASCII4> Пленума Высшего Арбитражного Суда РФ от 15 декабря 2004 г. N 29 "О некоторых вопросах практики применения Федерального закона "О несостоятельности (банкротстве)"'
  }
  : = {
   Handle = 2
   FirstIndent = 0
   Text = 'В обзоре судебной практики нашли отражение вопросы, связанные с применением арбитражными судами отдельных положений Федерального закона от 26 октября 2002 г. N 127-ФЗ "О несостоятельности (банкротстве)". В частности, разъясняется, что арест имущества должника, а также иные ограничения должника в части распоряжения принадлежащим ему имуществом могут быть применены лишь по определению арбитражного суда, рассматривающего дело о банкротстве должника.'
  }
  : = {
   Handle = 3
   FirstIndent = 0
   Text = 'Требования налоговых, таможенных и иных органов, в чью компетенцию входит взимание и взыскание соответствующих сумм платежей, по обязательным платежам, возникшим после принятия заявления о признании должника банкротом и до открытия конкурсного производства, а также по обязательным платежам, срок исполнения которых наступил после введения соответствующей процедуры банкротства (текущие платежи), удовлетворяются вне рамок дела о банкротстве.'
  }
  : = {
   Handle = 4
   FirstIndent = 0
   Text = 'Также разъясняются особенности применения судами норм закона, касающихся правового положения залоговых кредиторов при несостоятельности должника, а также наблюдения, финансового оздоровления, внешнего управления, конкурсного производства, мирового соглашения и упрощенных процедур банкротства.'
  }
  : = {
  }
 }
