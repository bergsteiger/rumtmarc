 [%f] set_var %( 'STR' %, [%f] str_replace %( %2 |N %, [%f] space %( %S )% %, [%f] empty %( %S )% )% )% 
//	%f_clear_list(SPLITED)\
 [%f] clear_list %( 'SPLITED' )% 
