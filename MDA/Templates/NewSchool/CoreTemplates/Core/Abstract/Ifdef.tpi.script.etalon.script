: in_diapason OBJECT IN %S
//	[{%f_string_cmp("%SN","%1N","\>")=1&%f_string_cmp("%SN","%2N","\<")=1}{[{%SN=%1N|%SN=%2N}{false}true]}true]
 %IF ( ( [%f] string_cmp %( %S |N %, %1 |N %, '>' )% ) %== 1 %&& 
   ( ( [%f] string_cmp %( %S |N %, %2 |N %, '<' )% ) %== 1  ) ) 
  %ELSE
   %IF ( ( %S |N ) %== ( %1 |N ) %|| 
     ( ( %S |N ) %== ( %2 |N )  ) ) 
    %ELSE
      false 
    %THEN
     true 
   %END-IF
  %THEN
   true 
 %END-IF

