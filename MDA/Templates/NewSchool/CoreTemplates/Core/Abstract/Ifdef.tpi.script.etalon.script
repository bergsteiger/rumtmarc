 %FOR %ITEM-CONDITION ( ( [%f] is_empty %( 'SPLITED' )% ) %==  false ) 
  ( 'W' ) 
  %IF-NOT-EMPTY
   [%f] pop_first_to_var %( 'SPLITED' %, 'ITEM' )% [%f] add_to_list %( ( get_global_var ( 'ID' ) |N ) %, ( get_global_var ( 'ITEM' ) |N ) )% 
  %END-IF
 %END-FOR
//	%f_clear_list(SPLITED)
 [%f] clear_list %( 'SPLITED' )% 

