   %END-IF
//	[{%{CURR_TOKEN}N=op}%f_set_var(OP,"%{OP}N%{CURR_SYMBOL}N")]\
   %IF ( ( ( get_global_var ( 'CURR_TOKEN' ) |N ) ) %== 'op' ) 
