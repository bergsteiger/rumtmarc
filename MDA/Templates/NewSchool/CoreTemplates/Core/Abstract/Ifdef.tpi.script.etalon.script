//	[{%{CURR_TOKEN}N=var2}%f_set_var(VAR2,"%{VAR2}N%{CURR_SYMBOL}N")]\
   %IF ( ( ( get_global_var ( 'CURR_TOKEN' ) |N ) ) %== 'var2' ) 
