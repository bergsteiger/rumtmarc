 %FOR %ITEM-CONDITION ( ( [%f] is_empty %( 'TEMP_IMPORT_LIST' )% ) %==  false ) 
  ( 'W' ) 
  [%f] pop_last_to_var %( 'TEMP_IMPORT_LIST' %, '_IMPORT_' )% 
