  %IF ( ( ( get_global_var ( 'CURR_PACK' ) |N ) ) %== ( ( get_global_var ( 'SELF_PACK' ) |N ) ) %|| 
    ( ( ( get_global_var ( 'CURR_PACK' ) |N ) ) %== ( ( get_global_var ( 'SELF_PATH' ) |N ) )  ) ) 
