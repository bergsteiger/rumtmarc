    [%f] add_to_list %( ( get_global_var ( 'IMPORT_CONTEXT' ) |U ) '_IMPORT_LIST' %, [%f] get_package_from_type %( %r %, ( get_global_var ( 'R_PATH' ) |N ) )% '.*' )% 
