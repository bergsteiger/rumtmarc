  %IF ( ( [%f] need_force_import %( ( get_global_var ( '_IMPORT_' ) ) %, ( get_global_var ( '_IMPORT_' ) |N ) )% ) %==  false %&& 
    ( ( [%f] is_package_import %( ( get_global_var ( '_IMPORT_' ) ) )% ) %==  false  ) %&& 
